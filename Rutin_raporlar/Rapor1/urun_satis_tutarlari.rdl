    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ay">
            <Value>=Parameters!ay.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@yil">
            <Value>=Parameters!yil.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@productid">
            <Value>=Parameters!productid.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
p.ProductID, p.ProductName,sum(od.Quantity*od.UnitPrice) as [Total], convert(nvarchar,o.OrderDate,103)
from [Order Details] od
inner join Products p on od.ProductID = p.ProductID
inner join Orders o on od.OrderID = o.OrderID
where month(o.OrderDate) in(@ay) and year(o.OrderDate) = @yil and p.ProductID in(@productid)
group by p.ProductName,p.ProductID,o.OrderDate, convert(nvarchar,o.OrderDate,103)
order by Total DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
