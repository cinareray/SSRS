          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OrderDate">
          <DataField>OrderDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@siparisno">
            <Value>=Parameters!siparisno.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select sum(od.Quantity*od.UnitPrice) as Total from  [Order Details] od 
inner join Products p on p.ProductID = od.ProductID
inner join Orders o on od.OrderID = o.OrderID
where o.OrderID in(@siparisno)</CommandText>
      </Query>
