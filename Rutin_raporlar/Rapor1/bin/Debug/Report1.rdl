  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@siparisno">
            <Value>=Parameters!siparisno.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select o.OrderID, string_agg(p.ProductName,'-') as [Urun_adi],string_agg(p.ProductID,' ') as [Urun_kodu], convert(nvarchar,o.OrderDate,103) as [OrderDate]
from  [Order Details] od 
inner join Products p on p.ProductID = od.ProductID
inner join Orders o on od.OrderID = o.OrderID
where  o.OrderID  in(@siparisno)

group by  o.OrderID, [OrderDate]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="OrderID">
          <DataField>OrderID</DataField>
